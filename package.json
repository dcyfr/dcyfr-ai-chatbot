{
  "name": "@dcyfr/ai-chatbot",
  "version": "1.0.0",
  "description": "Conversational AI chatbot template - DCYFR AI starter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chat": {
      "import": "./dist/chat/index.js",
      "types": "./dist/chat/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./streaming": {
      "import": "./dist/streaming/index.js",
      "types": "./dist/streaming/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "types": "./dist/plugins/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "example:simple": "tsx examples/simple-chat/index.ts",
    "example:streaming": "tsx examples/streaming-chat/index.ts",
    "example:advanced": "tsx examples/advanced-chat/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:run",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "ai",
    "chatbot",
    "conversational",
    "llm",
    "streaming",
    "dcyfr",
    "template",
    "starter"
  ],
  "author": "DCYFR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcyfr/dcyfr-ai-chatbot.git"
  },
  "homepage": "https://github.com/dcyfr/dcyfr-ai-chatbot#readme",
  "bugs": {
    "url": "https://github.com/dcyfr/dcyfr-ai-chatbot/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "@dcyfr/ai": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@dcyfr/ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.10",
    "@types/node": "^22.10.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^8.56.0",
    "globals": "^17.3.0",
    "tsx": "^4.19.0",
    "typescript": "~5.7.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
